{
  "name": "react-panel",
  "version": "0.0.1",
  "description": "React Panel example for Adobe CEP",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config webpack.config.js --mode development --open-page index.html",
    "build:host": "babel src/host/index.jsx --out-file dist/host/index.jsx --presets extendscript",
    "build:client": "webpack -d",
    "build:client:prod": "webpack -p",
    "copy:CSXS": "shx cp -r src/CSXS/**/* dist/CSXS",
    "copy:debug": "shx cp src/.debug dist/.debug",
    "build": "npm-run-all build:client:prod build:host copy:*"
  },
  "keywords": [
    "React",
    "CEP",
    "Panel",
    "Extension"
  ],
  "author": "Adobe",
  "license": "Apache-2.0",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-extendscript": "^0.1.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-include-assets-plugin": "^1.0.3",
    "html-webpack-plugin": "^3.0.4",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "precss": "^3.1.2",
    "shelljs": "^0.8.1",
    "shx": "^0.2.2",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "lorem-ipsum": "^1.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
